msgid "CREATE SESSION WITH ADMIN OPTION"
msgstr ""

#: ../../ref/databases.txt:731
msgid "CREATE TABLE WITH ADMIN OPTION"
msgstr ""

#: ../../ref/databases.txt:732
msgid "CREATE SEQUENCE WITH ADMIN OPTION"
