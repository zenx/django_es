msgid "`SAP SQL Anywhere`_"
msgstr ""

#: ../../ref/databases.txt:899
msgid "`IBM DB2`_"
msgstr ""

#: ../../ref/databases.txt:900
msgid "`Microsoft SQL Server`_"
msgstr ""

#: ../../ref/databases.txt:901
msgid "Firebird_"
msgstr ""

#: ../../ref/databases.txt:902
msgid "ODBC_"
msgstr ""

